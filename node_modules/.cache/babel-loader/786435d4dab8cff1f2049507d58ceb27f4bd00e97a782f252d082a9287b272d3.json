{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  computed: {\n    userData() {\n      return this.$store.state.userData;\n    },\n    employees() {\n      if (this.userData && this.userData.length > 0) {\n        // Filter unique employees based on empId\n        const uniqueEmployees = this.userData.reduce((acc, curr) => {\n          if (!acc.find(emp => emp.empId === curr.empId)) {\n            acc.push(curr);\n          }\n          return acc;\n        }, []);\n        return uniqueEmployees;\n      }\n      return [];\n    }\n  }\n};","map":{"version":3,"names":["computed","userData","$store","state","employees","length","uniqueEmployees","reduce","acc","curr","find","emp","empId","push"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\attendenceForm.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- User Table -->\r\n    <table v-if=\"userData && userData.length > 0\" class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>UserId</th>\r\n          <th>Name</th>\r\n          <th>Email</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr>\r\n          <td>{{ userData.userId }}</td>\r\n          <td>{{ userData.userName }}</td>\r\n          <td>{{ userData.email }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Employee Table -->\r\n    <table v-if=\"employees && employees.length > 0\" class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>EmployeeId</th>\r\n          <th>EmployeeName</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(employee, index) in employees\" :key=\"'emp_' + index\">\r\n          <td>{{ employee.empId }}</td>\r\n          <td>{{ employee.empName }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  computed: {\r\n    userData() {\r\n      return this.$store.state.userData;\r\n    },\r\n    employees() {\r\n      if (this.userData && this.userData.length > 0) {\r\n        // Filter unique employees based on empId\r\n        const uniqueEmployees = this.userData.reduce((acc, curr) => {\r\n          if (!acc.find(emp => emp.empId === curr.empId)) {\r\n            acc.push(curr);\r\n          }\r\n          return acc;\r\n        }, []);\r\n        return uniqueEmployees;\r\n      }\r\n      return [];\r\n    }\r\n  },\r\n}\r\n</script>\r\n"],"mappings":";AAuCA,eAAe;EACbA,QAAQ,EAAE;IACRC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,QAAQ;IACnC,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,IAAI,CAACH,QAAO,IAAK,IAAI,CAACA,QAAQ,CAACI,MAAK,GAAI,CAAC,EAAE;QAC7C;QACA,MAAMC,eAAc,GAAI,IAAI,CAACL,QAAQ,CAACM,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAK;UAC1D,IAAI,CAACD,GAAG,CAACE,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,KAAI,KAAMH,IAAI,CAACG,KAAK,CAAC,EAAE;YAC9CJ,GAAG,CAACK,IAAI,CAACJ,IAAI,CAAC;UAChB;UACA,OAAOD,GAAG;QACZ,CAAC,EAAE,EAAE,CAAC;QACN,OAAOF,eAAe;MACxB;MACA,OAAO,EAAE;IACX;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}