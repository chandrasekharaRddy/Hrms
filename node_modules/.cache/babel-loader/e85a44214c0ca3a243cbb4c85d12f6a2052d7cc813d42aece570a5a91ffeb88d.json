5px;\r\n  padding-right: 5px;\r\n  margin-left: 350px;\r\n}\r\n.DateActivity{\r\n  border: 1px solid;\r\n  width: 430px;\r\n  height: 340px;\r\n  padding-left: 50px;\r\n  padding-top: 20px;\r\n}\r\n\r\n\r\n.WeekBox{\r\n  display: inline-block;\r\n  border-left: 6px solid;\r\n  border-color: palevioletred;\r\n  width:180px;\r\nbox-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\r\nheight: 100px;\r\n}\r\n\r\n.MonthBox{\r\n  display: inline-block;\r\n  border-left: 6px solid;\r\n  border-color:rgb(70, 206, 70);\r\n  width:180px;\r\nmargin-left: 20px;\r\nheight: 100px;\r\nbox-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\r\n}\r\n.AbsentBox{\r\n  display: inline-block;\r\n  border-left: 6px solid;\r\n  border-color:rgb(231, 14, 14);\r\n  width:180px;\r\nmargin-left: 20px;\r\nbox-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\r\nheight: 100px;\r\nmargin-right: 10px;\r\n}\r\n.leavebalance1{\r\n  font-size: 30px;\r\n  text-align: left;\r\n  padding-left: 30px;\r\n  font-family:'Times New Roman', Times, serif;\r\n  color:  palevioletred;\r\n}\r\n\r\n.leavebalance2{\r\n  font-size: 30px;\r\n  text-align: left;\r\n  padding-left: 30px;\r\n  color: rgb(70, 206, 70);\r\n  font-family:'Times New Roman', Times, serif;\r\n}\r\n.leavebalance3{\r\n  font-size: 30px;\r\n  text-align: left;\r\n  padding-left: 30px;\r\n  color: rgb(231, 14, 14);\r\n  font-family:'Times New Roman', Times, serif;\r\n}\r\n\r\n.StatisticsContainer {\r\n  display: flex;\r\n  border-radius: 10px;\r\n  padding-bottom: 2px;\r\n  border: 1px solid;\r\n  width: 700px;\r\n  padding-top: 20px;\r\n  height: 150px;\r\n  padding-left: 20px;\r\n\r\n}\r\n.Heading{\r\n  padding-top: 10px;\r\n  padding-bottom: 5px;\r\n  font-size: 25px;\r\n  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;\r\n  color: rgb(214, 60, 32);\r\n}\r\n.AttendenceTable{\r\n  border: 1px solid;\r\n  margin-left: 10px;\r\n  padding-left: 20px;\r\n  padding-right: 20px;\r\n  margin-right: 10px;\r\n}\r\n\r\n\r\n</style>\r\n<!-- <template>\r\n