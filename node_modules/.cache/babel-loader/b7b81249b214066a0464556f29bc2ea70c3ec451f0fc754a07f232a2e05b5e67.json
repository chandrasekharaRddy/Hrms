able v-if=\"selectedDate\" class=\"table table-bordered\">\r\n      <!-- Goals Table Header -->\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>Name</th>\r\n          <th>Date</th>\r\n          <th>InTime</th>\r\n          <th>OutTime</th>\r\n        </tr>\r\n      </thead>\r\n      <!-- Goals Table Body -->\r\n      <tbody>\r\n        <tr v-for=\"(goal, index) in filteredGoals\" :key=\"index\">\r\n          <td>{{ goal.name }}</td>\r\n          <td>{{ goal.Date }}</td>\r\n          <td>{{ goal.InTime1 }}</td>\r\n          <td>{{ goal.OutTime1 }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      // Initialize employee and user data\r\n      employee: {},\r\n      user: {},\r\n      workingHours: [],\r\n      goals: [],\r\n      filteredGoals: [],\r\n      selectedDate: null,\r\n      dateVisible: true\r\n    };\r\n  },\r\n  computed: {\r\n    // Get user data from Vuex store\r\n    userData() {\r\n      return this.$store.getters.getUserData;\r\n    },\r\n    // Get employee data from Vuex store\r\n    employeeData() {\r\n      return this.$store.getters.getEmployeeData;\r\n    }\r\n  },\r\n  watch: {\r\n    // Watch for changes in user data\r\n    userData: {\r\n      handler(newValue) {\r\n        // Update user data\r\n        this.user = newValue;\r\n      },\r\n      immediate: true, // Call immediately upon component creation\r\n    },\r\n    // Watch for changes in employee data\r\n    employeeData: {\r\n      handler(newValue) {\r\n        // Update employee data\r\n        this.employee = newValue;\r\n      },\r\n      immediate: true, // Call immediately upon component creation\r\n    }\r\n  },\r\n  methods: {\r\n    // Calculate working hours from time pairs\r\n    calculateWorkingHours(...timePairs) {\r\n      let totalHours = 0;\r\n      for (let i = 0; i < timePairs.length; i += 2) {\r\n        const inTime = new Date(`2000-01-01T${timePairs[i]}`);\r\n        const outTime = new Date(`2000-01-01T${timePairs[i + 1]}`);\r\n        const diff = outTime - inTime;\r\n        const hours = diff / (1000 * 60 * 60);\r\n        totalHours += hours;\r\n      }\r\n      return totalHours.toFixed(2);\r\n    },\r\n    // Handle click on date to filter goals\r\n    getDate(date) {\r\n      if (this.selectedDate === date) {\r\n        this.selectedDate = null; \r\n      } else {\r\n        this.selectedDate = date;\r\n        this.filteredGoals = this.goals.filter((goal) => goal.Date === date);\r\n        this.dateVisible = false;\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Add your styles here */\r\n</style>\r\n"],"mappings":"AAoFA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,QAAQ,EAAE,CAAC,CAAC;MACZC,IAAI,EAAE,CAAC,CAAC;MACRC,YAAY,EAAE,EAAE;MAChBC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE,IAAI;MAClBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,QAAQA,CAAA,EAAG;MACT,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAACC,WAAW;IACxC,CAAC;IACD;IACAC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACH,MAAM,CAACC,OAAO,CAACG,eAAe;IAC5C;EACF,CAAC;EACDC,KAAK,EAAE;IACL;IACAN,QAAQ,EAAE;MACRO,OAAOA,CAACC,QAAQ,EAAE;QAChB;QACA,IAAI,CAACf,IAAG,GAAIe,QAAQ;MACtB,CAAC;MACDC,SAAS,EAAE,IAAI,CAAE;IACnB,CAAC;IACD;IACAL,YAAY,EAAE;MACZG,OAAOA,CAACC,QAAQ,EAAE;QAChB;QACA,IAAI,CAAChB,QAAO,GAAIgB,QAAQ;MAC1B,CAAC;MACDC,SAAS,EAAE,IAAI,CAAE;IACnB;EACF,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,qBAAqBA,CAAC,GAAGC,SAAS,EAAE;MAClC,IAAIC,UAAS,GAAI,CAAC;MAClB,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIF,SAAS,CAACG,MAAM,EAAED,CAAA,IAAK,CAAC,EAAE;QAC5C,MAAME,MAAK,GAAI,IAAIC,IAAI,CAAE,cAAaL,SAAS,CAACE,CAAC,CAAE,EAAC,CAAC;QACrD,MAAMI,OAAM,GAAI,IAAID,IAAI,CAAE,cAAaL,SAAS,CAACE,CAAA,GAAI,CAAC,CAAE,EAAC,CAAC;QAC1D,MAAMK,IAAG,GAAID,OAAM,GAAIF,MAAM;QAC7B,MAAMI,KAAI,GAAID,IAAG,IAAK,IAAG,GAAI,EAAC,GAAI,EAAE,CAAC;QACrCN,UAAS,IAAKO,KAAK;MACrB;MACA,OAAOP,UAAU,CAACQ,OAAO,CAAC,CAAC,CAAC;IAC9B,CAAC;IACD;IACAC,OAAOA,CAACC,IAAI,EAAE;MACZ,IAAI,IAA