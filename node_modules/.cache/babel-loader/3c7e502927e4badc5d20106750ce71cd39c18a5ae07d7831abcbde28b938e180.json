{"ast":null,"code":"export default {\n  computed: {\n    userDetails() {\n      return this.$store.state.userData && this.$store.state.userData.length > 0 ? this.$store.state.userData[0] : null;\n    },\n    employeeList() {\n      if (this.$store.state.userData && this.$store.state.userData.length > 0) {\n        return this.$store.state.userData.slice(1).map(employee => ({\n          empId: employee.empId,\n          empName: employee.empName\n        }));\n      }\n      return [];\n    },\n    selectedEmployeeDetails() {\n      if (this.selectedEmployee) {\n        return this.$store.state.userData.find(employee => employee.empId === this.selectedEmployee);\n      }\n      return null;\n    }\n  },\n  data() {\n    return {\n      selectedEmployee: null\n    };\n  },\n  methods: {\n    showEmployeeDetails(empId) {\n      console.log(\"Selected Employee ID:\", empId);\n      this.selectedEmployee = empId;\n    }\n  }\n};","map":{"version":3,"names":["computed","userDetails","$store","state","userData","length","employeeList","slice","map","employee","empId","empName","selectedEmployeeDetails","selectedEmployee","find","data","methods","showEmployeeDetails","console","log"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\attendenceForm.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- User Table -->\r\n    <table v-if=\"userDetails\" class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>UserId</th>\r\n          <th>Name</th>\r\n          <th>Email</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr>\r\n          <td>{{ userDetails.userId }}</td>\r\n          <td>{{ userDetails.userName }}</td>\r\n          <td>{{ userDetails.email }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Employee Table -->\r\n    <table v-if=\"employeeList && employeeList.length > 0\" class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>EmployeeId</th>\r\n          <th>EmployeeName</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(employee, index) in employeeList\" :key=\"'emp_' + index\">\r\n          <td @click=\"showEmployeeDetails(employee.empId)\">{{ employee.empId }}</td>\r\n          <td>{{ employee.empName }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Employee Details Table -->\r\n    <div v-if=\"selectedEmployee\">\r\n      <table class=\"table table-bordered\">\r\n        <thead class=\"table-dark\">\r\n          <tr>\r\n            <th>EmployeeId</th>\r\n            <th>Name</th>\r\n\r\n            <th>Date</th>\r\n            <th>Intime</th>\r\n            <th>Outtime</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <template v-if=\"selectedEmployeeDetails\">\r\n            <tr>\r\n              <td>{{ selectedEmployeeDetails.empId }}</td>\r\n              <td>{{ selectedEmployeeDetails.empName }}</td>\r\n              \r\n              <td v-for=\"attendance in selectedEmployeeDetails.attendance\" :key=\"attendance.date\">\r\n                console.log(\"ðŸš€ ~ selectedEmployeeDetails.attendance:\", selectedEmployeeDetails.attendance)\r\n                {{ attendance.date }}\r\n              </td>\r\n              <td v-for=\"attendance in selectedEmployeeDetails.attendance\" :key=\"attendance.date\">\r\n                <span v-for=\"intime in attendance.inTimes\" :key=\"intime\">{{ inTime }}</span>\r\n              </td>\r\n              <td v-for=\"attendance in selectedEmployeeDetails.attendance\" :key=\"attendance.date\">\r\n                <span v-for=\"outtime in attendance.outTimes\" :key=\"outtime\">{{ outTime }}</span>\r\n              </td>\r\n            </tr>\r\n          </template>\r\n          <template v-else>\r\n            <tr>\r\n              <td colspan=\"6\">No details available for the selected employee.</td>\r\n            </tr>\r\n          </template>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  computed: {\r\n    userDetails() {\r\n      return this.$store.state.userData && this.$store.state.userData.length > 0\r\n        ? this.$store.state.userData[0]\r\n        : null;\r\n    },\r\n    employeeList() {\r\n      if (this.$store.state.userData && this.$store.state.userData.length > 0) {\r\n        return this.$store.state.userData.slice(1).map(employee => ({\r\n          empId: employee.empId,\r\n          empName: employee.empName\r\n        }));\r\n      }\r\n      return [];\r\n    },\r\n    selectedEmployeeDetails() {\r\n      if (this.selectedEmployee) {\r\n        return this.$store.state.userData.find(employee => employee.empId === this.selectedEmployee);\r\n      }\r\n      return null;\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      selectedEmployee: null\r\n    };\r\n  },\r\n  methods:{\r\n    showEmployeeDetails(empId) {\r\n      console.log(\"Selected Employee ID:\", empId);\r\n      this.selectedEmployee = empId;\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA+EA,eAAe;EACbA,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,QAAO,IAAK,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,QAAQ,CAACC,MAAK,GAAI,IACrE,IAAI,CAACH,MAAM,CAACC,KAAK,CAACC,QAAQ,CAAC,CAAC,IAC5B,IAAI;IACV,CAAC;IACDE,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACJ,MAAM,CAACC,KAAK,CAACC,QAAO,IAAK,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,QAAQ,CAACC,MAAK,GAAI,CAAC,EAAE;QACvE,OAAO,IAAI,CAACH,MAAM,CAACC,KAAK,CAACC,QAAQ,CAACG,KAAK,CAAC,CAAC,CAAC,CAACC,GAAG,CAACC,QAAO,KAAM;UAC1DC,KAAK,EAAED,QAAQ,CAACC,KAAK;UACrBC,OAAO,EAAEF,QAAQ,CAACE;QACpB,CAAC,CAAC,CAAC;MACL;MACA,OAAO,EAAE;IACX,CAAC;IACDC,uBAAuBA,CAAA,EAAG;MACxB,IAAI,IAAI,CAACC,gBAAgB,EAAE;QACzB,OAAO,IAAI,CAACX,MAAM,CAACC,KAAK,CAACC,QAAQ,CAACU,IAAI,CAACL,QAAO,IAAKA,QAAQ,CAACC,KAAI,KAAM,IAAI,CAACG,gBAAgB,CAAC;MAC9F;MACA,OAAO,IAAI;IACb;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,gBAAgB,EAAE;IACpB,CAAC;EACH