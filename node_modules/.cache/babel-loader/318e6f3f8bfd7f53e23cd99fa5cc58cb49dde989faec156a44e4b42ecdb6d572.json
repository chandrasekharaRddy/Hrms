{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      users: []\n    };\n  },\n  methods: {\n    login() {\n      axios.get(\"https://localhost:7135/api/Data\").then(response => {\n        this.users = response.data.users;\n        const user = this.users.find(u => u.userName === this.username && u.password === this.password);\n        if (user) {\n          alert(`userName:${user.userName}`);\n          const userData = {\n            name: user.name,\n            Date: user.date,\n            InTime1: user.intime,\n            OutTime1: user.outtime\n          };\n          // console.log(userData.name)\n          this.$store.dispatch('setUserData', userData);\n          this.$router.push({\n            name: 'navigation'\n          });\n          console.log(userData);\n        } else {\n          alert(\"Wrong credentials\");\n        }\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","users","methods","login","get","then","response","user","find","u","userName","alert","userData","name","Date","date","InTime1","intime","OutTime1","outtime","$store","dispatch","$router","push","console","log","catch","error"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\logiForm.vue"],"sourcesContent":["  <template>\r\n  <div class=\"login-box\">\r\n    <form @submit.prevent=\"login\">\r\n      <div class=\"user-box\">\r\n      <input type=\"text\" name=\"\"  v-model=\"username\" >\r\n      <label>Username</label>\r\n    </div>\r\n      <div class=\"user-box\">\r\n      <input type=\"password\" name=\"\"  v-model=\"password\" >\r\n      <label>Password</label>\r\n    </div>\r\n      <br>\r\n  \r\n      <a href=\"#\">\r\n      <span></span>\r\n      <span></span>\r\n      <span></span>\r\n      <span></span>\r\n      <button class=\"button\">Login</button>\r\n    </a></form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        username: \"\",\r\n        password: \"\",\r\n        users: []\r\n      }\r\n    },\r\n    methods: {\r\n      login() {\r\n        axios.get(\"https://localhost:7135/api/Data\")\r\n          .then(response => {\r\n            this.users = response.data.users;\r\n            \r\n            const user = this.users.find(u => u.userName === this.username && u.password === this.password);\r\n            if (user) {\r\n              alert(`userName:${user.userName}`)\r\n              const userData =  {\r\n                name: user.name,\r\n                  Date: user.date,\r\n                  InTime1: user.intime,\r\n                  OutTime1: user.outtime,\r\n        \r\n              };\r\n             // console.log(userData.name)\r\n              this.$store.dispatch('setUserData', userData);\r\n             this.$router.push({ name: 'navigation' });\r\n              console.log(userData);\r\n            } else {\r\n              alert(\"Wrong credentials\");\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching data:', error);\r\n          });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n<style>\r\nhtml {\r\n  height: 100%;\r\n}\r\nbody {\r\n  margin:0;\r\n  padding:0;\r\n  font-family: sans-serif;\r\n  background: linear-gradient(#141e30,#141e30 );\r\n}\r\n\r\n.login-box {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  width: 400px;\r\n  padding: 40px;\r\n  transform: translate(-50%, -50%);\r\n  background: #141e30;\r\n  box-sizing: border-box;\r\n  box-shadow: 0 15px 25px rgba(148, 180, 106, 0.6);\r\n  border-radius: 10px;\r\n}\r\n\r\n.login-box h2 {\r\n  margin: 0 0 30px;\r\n  padding: 0;\r\n  color: #833f3f;\r\n  text-align: center;\r\n}\r\n\r\n.login-box .user-box {\r\n  position: relative;\r\n}\r\n\r\n.login-box .user-box input {\r\n  width: 100%;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #fff;\r\n  margin-bottom: 30px;\r\n  border: none;\r\n  border-bottom: 1px solid #fff;\r\n  outline: none;\r\n  background: transparent;\r\n}\r\n.login-box .user-box label {\r\n  position: absolute;\r\n  top:0;\r\n  left: 0;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #cc6b6b;\r\n  pointer-events: none;\r\n  transition: .5s;\r\n}\r\n\r\n.login-box .user-box input:focus ~ label,\r\n.login-box .user-box input:valid ~ label {\r\n  top: -20px;\r\n  left: 0;\r\n  color: #03e9f4;\r\n  font-size: 12px;\r\n}\r\n\r\n.login-box form a {\r\n  position: relative;\r\n  display: inline-block;\r\n  padding: 10px 20px;\r\n  color: #03e9f4;\r\n  font-size: 16px;\r\n  text-decoration: none;\r\n  text-transform: uppercase;\r\n  overflow: hidden;\r\n  transition: .5s;\r\n  margin-top: 40px;\r\n  letter-spacing: 4px\r\n}\r\n\r\n.login-box a:hover {\r\n  background: #03e9f4;\r\n  color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 5px #03e9f4,\r\n              0 0 25px #03e9f4,\r\n              0 0 50px #03e9f4,\r\n              0 0 100px #03e9f4;\r\n}\r\n\r\n.login-box a span {\r\n  position: absolute;\r\n  display: block;\r\n}\r\n\r\n.login-box a span:nth-child(1) {\r\n  top: 0;\r\n  left: -100%;\r\n  width: 100%;\r\n  height: 2px;\r\n  background: linear-gradient(90deg, transparent, #03e9f4);\r\n  animation: btn-anim1 1s linear infinite;\r\n}\r\n\r\n@keyframes btn-anim1 {\r\n  0% {\r\n    left: -100%;\r\n  }\r\n  50%,100% {\r\n    left: 100%;\r\n  }\r\n}\r\n\r\n.login-box a span:nth-child(2) {\r\n  top: -100%;\r\n  right: 0;\r\n  width: 2px;\r\n  height: 100%;\r\n  background: linear-gradient(180deg, transparent, #03e9f4);\r\n  animation: btn-anim2 1s linear infinite;\r\n  animation-delay: .25s\r\n}\r\n\r\n@keyframes btn-anim2 {\r\n  0% {\r\n    top: -100%;\r\n  }\r\n  50%,100% {\r\n    top: 100%;\r\n  }\r\n}\r\n\r\n.log