dy>\r\n      </table>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  data() {\r\n    return {\r\n      normalLeave: 12,\r\n      sickLeave: 12,\r\n      form: {\r\n        name: \"\",\r\n        textArea: \"\",\r\n        leave: \"\",\r\n        date1:\"\",\r\n      date2: \"\",\r\n      },\r\n      goals: [],\r\n      users:'',\r\n    };\r\n     \r\n  },\r\n  methods: {\r\n    submit() {\r\n      axios.post(\"https://localhost:7188/api/User/createleave\", {\r\n        emp_id: this.form.id,\r\n        emp_name: this.form.name,\r\n        leaveType: this.form.leave,\r\n        startDate: this.form.date1,\r\n        endDate: this.form.date2,\r\n        days: 6, // or calculate days difference between dates\r\n        status: this.form.reason\r\n      })\r\n      .then(response => {\r\n        this.goals.push(response.data); // assuming the response is an object containing the leave data\r\n        console.log(this.goals);\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error adding leave:\", error);\r\n      });\r\n    }\r\n}\r\n};\r\n</script> \r\n<style>\r\n.form {\r\n  max-width: 400px;\r\n  margin: auto;\r\n  font-family: 'Arial', sans-serif;\r\n}\r\n\r\n.form div {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  margin-bottom: 16px;\r\n}\r\n\r\n.form label {\r\n  flex: 1;\r\n  margin-right: 10px;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textArea {\r\n  flex: 2;\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.form button {\r\n  background-color: #4caf50;\r\n  color: white;\r\n  padding: 10px 15px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.form button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.select {\r\n  width: 250px;\r\n  height: 40px;\r\n  border-radius: 10px;\r\n  margin: 5px;\r\n  display: inline-block;\r\n}\r\n\r\n.leaves {\r\n  /* border: 1px solid; */\r\n  width: 500px;\r\n  display: flex