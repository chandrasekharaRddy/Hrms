ry {\r\n        await this.$store.dispatch('login', { username: this.username, password: this.password });\r\n        // Redirect user after successful login\r\n        this.$router.push({ name: 'navigation' });\r\n      } catch (error) {\r\n        window.alert(error.message);\r\n      }\r\n    },\r\n  },\r\n\r\n    TeamMembers(){\r\n      if(this.username === \"praveen\" && this.password === \"praveen@123\"){\r\n        this.$store.commit('teammembers',true);\r\n      }\r\n      else if(this.username === \"surya\" && this.password === \"surya@123\"){\r\n        this.$store.commit('teammembers',true);\r\n      }\r\n      else{\r\n        this.$store.commit('teammembers',false);\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style>\r\nhtml, body {\r\n  height: 100%;\r\n  margin-top: 0px;\r\n  padding-top: 0px;\r\n \r\n \r\n\r\n}\r\n\r\n.login-box {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100vh; \r\n  background: url(https://d2slcw3kip6qmk.cloudfront.net/marketing/blog/2017Q4/hr-matrix/hr-matrix-header.png) center/cover;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.login-box form {\r\n  width: 400px;\r\n  padding: 40px;\r\n  background: transparent;\r\n  box-sizing: border-box;\r\n  border: 2px solid rgba(255,255,255,.2);\r\n\r\n  border-radius: 10px;\r\n}\r\n\r\n.user-box {\r\n  position: relative;\r\n}\r\n\r\n.user-box input {\r\n  width: 100%;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #fff;\r\n  margin-bottom: 30px;\r\n  border: none;\r\n  border-bottom: 1px solid #fff;\r\n  outline: none;\r\n  background: transparent;\r\n}\r\n\r\n.user-box label {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #cc6b6b;\r\n  pointer-events: none;\r\n  transition: .5s;\r\n}\r\n\r\n.user-box input:focus ~ label,\r\n.user-box input:valid ~ label {\r\n  top: -20px;\r\n  left: 0;\r\n  color: #03e9f4;\r\n  font-size: 12px;\r\n}\r\n\r\n.button1 {\r\n  wi