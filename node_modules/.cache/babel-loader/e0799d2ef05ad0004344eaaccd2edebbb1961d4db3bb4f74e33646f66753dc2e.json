g(\"Leave Details:\", leaveDetails);\r\n        this.goals.push({ leaveDetails, day: diff });\r\n\r\n        if (this.form.leave === \"Normal Leave\" && diff > 0) {\r\n      this.normalLeave -= diff;\r\n    } else if (this.form.leave === \"Sick Leave\" && diff > 0) {\r\n      this.sickLeave -= diff;\r\n    }\r\n\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error adding leave:\", error);\r\n        \r\n      });\r\n    },\r\n    \r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.form {\r\n  font-family: 'Arial', sans-serif;\r\n  width: 600px;\r\n  background: transparent;\r\n  margin-left: 500px; \r\n  margin-right: auto;\r\n  padding: 20px;\r\n}\r\n.form div {\r\ndisplay: flex;\r\nflex-direction: row;\r\nalign-items: center;\r\nmargin-bottom: 16px;\r\npadding-top: 10px;\r\nwidth: 400px;\r\ncolor:#000;\r\nmargin-right: 150px;\r\n}\r\n\r\n.form label {\r\ndisplay: block;\r\nmargin-right: 3px; \r\nmargin-top: 4px;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textArea {\r\nwidth: 80%;\r\npadding: 10px; \r\nmargin-bottom: 3px; \r\nborder: 1px solid #ccc;\r\nborder-radius: 4px;\r\n}\r\n\r\n.form button {\r\nbackground-color: #4caf50;\r\ncolor: white;\r\npadding: 2px 5px;\r\nborder: none;\r\nborder-radius: 4px;\r\ncursor: pointer;\r\nmargin-left: -250px;\r\n}\r\n\r\n.form button:hover {\r\nbackground-color: #45a049;\r\n}\r\n\r\n.select {\r\nwidth: 250px;\r\nheight: 40px;\r\nborder-radius: 10px;\r\nmargin: 5px;\r\ndisplay: inline-block;\r\n}\r\n\r\n.leaves {\r\n\r\nwidth: 500px;\r\ndisplay: flex;\r\nheight: 150px;\r\npadding-top: 20px;\r\npadding-left: 80px;\r\nmargin-left: 40px;\r\njustify-content: space-between;\r\n}\r\n\r\n.leavesBox1 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nwidth: 150px;\r\nheight: 140px;\r\n}\r\n\r\n.leavesBox2 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nmargin-right: 80px;\r\nwidth: 150px;\r\nheight: 140px;\r\n\r\n}\r\n\r\