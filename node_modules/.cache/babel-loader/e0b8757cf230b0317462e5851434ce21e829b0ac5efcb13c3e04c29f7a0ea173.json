 id=\"app\">\r\n    <h1>Create Leave Request</h1>\r\n    <form @submit.prevent=\"submitLeave\">\r\n      <label for=\"leaveType\">Leave Type:</label>\r\n      <input type=\"text\" id=\"leaveType\" v-model=\"leaveType\" required><br><br>\r\n\r\n      <label for=\"startDate\">Start Date:</label>\r\n      <input type=\"date\" id=\"startDate\" v-model=\"startDate\" required><br><br>\r\n\r\n      <label for=\"endDate\">End Date:</label>\r\n      <input type=\"date\" id=\"endDate\" v-model=\"endDate\" required><br><br>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data(){\r\n    return{\r\n      leaveType:'',\r\n      startDate:'',\r\n      endDate:''\r\n    }\r\n  },\r\n  computed: {\r\n    getUsername() {\r\n      return this.$store.getters.getUsername;\r\n    },\r\n    getPassword() {\r\n      return this.$store.getters.getPassword;\r\n    }\r\n  },\r\n methods:{\r\n  async submitLeave(){\r\n    console.log('Data to be stored:', { username: this.username, password: this.password });\r\n    try {\r\n        const response = await axios.post('https://yourapi.com/api/user/createleave', {\r\n          LeaveType: this.leaveType,\r\n          StartDate: this.startDate,\r\n          EndDate: this.endDate\r\n        }, {\r\n          params: {\r\n            userName: this.getUsername,\r\n            password: this.getPassword\r\n          }\r\n        });\r\n\r\n        console.log(response.data);\r\n        alert('Leave request created successfully!');\r\n      } catch (error) {\r\n        console.error(error);\r\n        alert('An error occurred while creating leave request.');\r\n      }\r\n  }\r\n }\r\n};\r\n</script>"],"mappings":"AAiRA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,SAAS,EAAC,EAAE;MACZC,SAAS,EAAC,EAAE;MACZC,OAAO,EAAC;IACV;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAACF,WAAW