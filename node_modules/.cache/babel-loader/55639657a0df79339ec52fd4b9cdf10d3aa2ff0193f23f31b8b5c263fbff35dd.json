ssword\" >\r\n        <label>Password</label>\r\n      </div>\r\n      <br>\r\n      <button class=\"button1\">Login</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      userName: \"\",\r\n      password: \"\",\r\n      userData:''\r\n    }\r\n  },\r\n  methods: {\r\n    async login() {\r\n      try {\r\n        const response = await axios.get('https://localhost:7188/api/User/authenticate', {\r\n        params: {\r\n          userName: this.userName,\r\n          password: this.password\r\n        }\r\n      })\r\n        const userData = response.data;\r\n        console.log(\"ðŸš€ ~ login ~ userData:\", userData);\r\n        this.$store.dispatch('setUserData', userData);\r\n        this.teamMembers();\r\n        this.$router.push({ name: 'navigation' });\r\n        \r\n      } catch (error) {\r\n        console.error('Error authenticating user:', error);\r\n        window.alert(\"Invalid username or password. Please try again.\");\r\n      }\r\n    },\r\n    teamMembers() {\r\n      if (this.userName === \"praveen\" && this.password === \"praveen@123\") {\r\n        this.$store.commit('teammembers', true);\r\n      } else if (this.userName === \"surya\" && this.password === \"surya@123\") {\r\n        this.$store.commit('teammembers', true);\r\n      } else {\r\n        this.$store.commit('teammembers', false);\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style>\r\nhtml, body {\r\n  height: 100%;\r\n  margin-top: 0px;\r\n  padding-top: 0px;\r\n}\r\n.login-box {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100vh; \r\n  background: url(https://d2slcw3kip6qmk.cloudfront.net/marketing/blog/2017Q4/hr-matrix/hr-matrix-header.png) center/cover;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.login-box form {\r\n  width: 400px;\r\n  padding: 40px;\r\n  background: transparent;\r\n  box-sizing: border-box;\r\n  border: 2px solid rgba(255,255,255,.2);\r\n\r\n  border-radius: 10px;\r\n}\r\n\r\n.user-box {\r\n  position: relative;\r\n}\r\n\r\n.user-box input {\r\n  width: 100%;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #fff;\r\n  margin-bottom: 30px;\r\n  border: none;\r\n  border-bottom: 1px solid #fff;\r\n  outline: none;\r\n  background: transparent;\r\n}\r\n.user-box label {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #cc6b6b;\r\n  pointer-events: none;\r\n  transition: .5s;\r\n}\r\n\r\n.user-box input:focus ~ label,\r\n.user-box input:valid ~ label {\r\n  top: -20px;\r\n  left: 0;\r\n  color: #03e9f4;\r\n  font-size: 12px;\r\n}\r\n\r\n.button1 {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  background-color: rgb(104, 185, 238);\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 40px;\r\n  box-shadow: 0 0 10px rgba(0,0,0,.1);\r\n  cursor: pointer;\r\n  font-weight: 600;\r\n  color: #333;\r\n}\r\n\r\n.button:hover {\r\n  background-color: darkgreen;\r\n}\r\n.title1{\r\n  color: white;\r\n  padding-left: 115px;\r\n  font-family:'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif\r\n}\r\n</style>  \r\n"],"mappings":";AAoBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAC;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,8CAA8C,EAAE;UACjFC,MAAM,EAAE;YACNP,QAAQ,EAAE,IAAI,CAACA,QAAQ;YACvBC,QAAQ,EAAE,IAAI,CAACA;UACjB;QACF,CAAC;QACC,MAAMC,QAAO,GAAIG,QAAQ,CAACN,IAAI;QAC9BS,OAAO,CAACC,GAAG,CAAC,wBAAwB,EAAEP,QAAQ,CAAC;QAC/C,IAAI,CAACQ,MAAM,CAACC,QAAQ,CAAC,aAAa,EAAET,QAAQ,CAAC;QAC7C,IAAI,CAACU,WAAW,CAAC,CAAC;QAClB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAa,CAAC,CAAC;MAE3C,EAAE,OAAOC,KAAK,EAAE;QACdR,OAAO,CAACQ,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;QAClDC,MAAM,CAACC,KAAK,CAAC,iDAAiD,CAAC;MACjE;IACF,CAAC;IACDN,WAAWA,CAAA,EAAG;MACZ,