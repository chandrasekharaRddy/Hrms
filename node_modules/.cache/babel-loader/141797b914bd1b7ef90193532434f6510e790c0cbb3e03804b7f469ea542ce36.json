{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      form: {\n        employeeName: '',\n        leaveType: '',\n        startDate: '',\n        endDate: ''\n      },\n      leaveRequestSubmitted: false,\n      teamLeaderApproval: false,\n      goals: []\n    };\n  },\n  methods: {\n    submitLeaveRequest() {\n      const start = new Date(this.form.startDate);\n      const end = new Date(this.form.endDate);\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1;\n      this.goals.push({\n        employeeName: this.form.employeeName,\n        leave: this.form.leaveType,\n        days: diff,\n        reason: 'Your Reason Here',\n        // Replace with the actual reason\n        teamLeaderApproval: false\n      });\n      this.leaveRequestSubmitted = true;\n    },\n    requestTeamLeaderApproval(index) {\n      if (index !== undefined) {\n        alert(`Approval request sent to Team Leader for ${this.goals[index].employeeName}`);\n        this.$set(this.goals, index, {\n          ...this.goals[index],\n          teamLeaderApproval: true\n        });\n      } else {\n        alert('Approval request sent to Team Leader');\n        this.teamLeaderApproval = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","form","employeeName","leaveType","startDate","endDate","leaveRequestSubmitted","teamLeaderApproval","goals","methods","submitLeaveRequest","start","Date","end","diff","push","leave","days","reason","requestTeamLeaderApproval","index","undefined","alert","$set"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\leaveTask.vue"],"sourcesContent":["<template>\r\n  <div class=\"\">\r\n    <form @submit.prevent=\"submitLeaveRequest\">\r\n      <label for=\"employeeName\">Employee Name:</label>\r\n      <input v-model=\"form.employeeName\" type=\"text\" />\r\n\r\n      <label for=\"leaveType\">Leave Type:</label>\r\n      <select v-model=\"form.leaveType\">\r\n        <option value=\"sick\">Sick Leave</option>\r\n        <option value=\"personal\">Personal Leave</option>\r\n      </select>\r\n\r\n      <label for=\"startDate\">Start Date:</label>\r\n      <input v-model=\"form.startDate\" type=\"date\" />\r\n\r\n      <label for=\"endDate\">End Date:</label>\r\n      <input v-model=\"form.endDate\" type=\"date\" />\r\n\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n\r\n    <div>\r\n      <table class=\"table table-bordered\">\r\n        <thead class=\"table-dark\">\r\n          <tr>\r\n            <th>Employee Name</th>\r\n            <th>Leave Type</th>\r\n            <th>Days</th>\r\n            <th>Reason</th>\r\n            <th>Action</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(goal, index) in goals\" :key=\"index\">\r\n            <td>{{ goal.employeeName }}</td>\r\n            <td>{{ goal.leave }}</td>\r\n            <td>{{ goal.days }}</td>\r\n            <td>{{ goal.reason }}</td>\r\n            <td>\r\n              <button v-if=\"!goal.teamLeaderApproval\" @click=\"requestTeamLeaderApproval(index)\">\r\n                Request Approval\r\n              </button>\r\n              <span v-else>Approved by Team Leader</span>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <button v-if=\"leaveRequestSubmitted && !teamLeaderApproval\" @click=\"requestTeamLeaderApproval\">\r\n      Request Team Leader Approval\r\n    </button>\r\n\r\n    <div v-if=\"teamLeaderApproval\">\r\n      <p><strong>Team Leader Approval:</strong> Approved</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        employeeName: '',\r\n        leaveType: '',\r\n        startDate: '',\r\n        endDate: '',\r\n      },\r\n      leaveRequestSubmitted: false,\r\n      teamLeaderApproval: false,\r\n      goals: [],\r\n    };\r\n  },\r\n  methods: {\r\n    submitLeaveRequest() {\r\n      const start = new Date(this.form.startDate);\r\n      const end = new Date(this.form.endDate);\r\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1;\r\n\r\n      this.goals.push({\r\n        employeeName: this.form.employeeName,\r\n        leave: this.form.leaveType,\r\n        days: diff,\r\n        reason: 'Your Reason Here', // Replace with the actual reason\r\n        teamLeaderApproval: false,\r\n      });\r\n\r\n      this.leaveRequestSubmitted = true;\r\n    },\r\n    requestTeamLeaderApproval(index) {\r\n      if (index !== undefined) {\r\n        alert(`Approval request sent to Team Leader for ${this.goals[index].employeeName}`);\r\n        this.$set(this.goals, index, { ...this.goals[index], teamLeaderApproval: true });\r\n      } else {\r\n        alert('Approval request sent to Team Leader');\r\n        this.teamLeaderApproval = true;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AA4DA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,YAAY,EAAE,EAAE;QAChBC,SAAS,EAAE,EAAE;QACbC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE;MACX,CAAC;MACDC,qBAAqB,EAAE,KAAK;MAC5BC,kBAAkB,EAAE,KAAK;MACzBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,IAAI,CAACX,IAAI,CAACG,SAAS,CAAC;MAC3C,MAAMS,GAAE,GAAI,IAAID,IAAI,CAAC,IAAI,CAACX,IAAI,CAACI,OAAO,CAAC;MACvC,MAAMS,IAAG,GAAI,CAACD,GAAE,GAAIF,KAAK,KAAK,IAAG,GAAI,EAAC,GAAI,EAAC,GAAI,EAAE,IAAI,CAAC;MAEtD,IAAI,CAACH,KAAK,CAACO,IAAI,CAAC;QACdb,YAAY,EAAE,IAAI,CAACD,IAAI,CAACC,YAAY;QACpCc,KAAK,EAAE,IAAI,CAACf,IAAI,CAACE,SAAS;QAC1Bc,IAAI,EAAEH,IAAI;QACVI,MAAM,EAAE,kBAAkB;QAAE;QAC5BX,kBAAkB,EAAE;MACtB,CAAC,CAAC;MAEF,IAAI,CAACD,qBAAoB,GAAI,IAAI;IACnC,CAAC;IACDa,yBAAyBA,CAACC,KAAK,EAAE;MAC/B,IAAIA,KAAI,KAAMC,SAAS,EAAE;QACvBC,KAAK,CAAE,4CAA2C,IAAI,CAACd,KAAK,CAACY,KAAK,CAAC,CAAClB,YAAa,EAAC,CAAC;QACnF,IAAI,CAACqB,IAAI,CAAC,IAAI,CAACf,KAAK,EAAEY,KAAK,EAAE;UAAE,GAAG,IAAI,CAACZ,KAAK,CAACY,KAAK,CAAC;UAAEb,kBAAkB,EAAE;QAAK,CAAC,