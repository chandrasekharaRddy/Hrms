normalLeave\">{{ normalLeave }}</div>\r\n      </div>\r\n      <div class=\"leavesBox\">\r\n        <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" fill=\"currentColor\" class=\"bi bi-calendar\" viewBox=\"0 0 16 16\">\r\n          <path d=\"M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5M1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4z\" />\r\n        </svg>\r\n        <label class=\"label\">Sick Leaves</label>\r\n        <div class=\"sickLeave\">{{ sickLeave }}</div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"form\">\r\n      <form @submit.prevent=\"submit\">\r\n        <div><label>Id:</label><input type=\"text\" v-model=\"form.id\"></div><br>\r\n        <div><label>Name:</label><input type=\"text\" v-model=\"form.name\"></div><br>\r\n        <div>\r\n          <label>Type Leave:</label>\r\n          <select class=\"select\" v-model=\"form.leave\">\r\n            <option>Normal Leave</option>\r\n            <option>Sick Leave</option>\r\n          </select>\r\n        </div><br>\r\n        <div><label>From:</label><input type=\"date\" v-model=\"form.date1\"></div><br>\r\n        <div><label>To:</label><input type=\"date\" v-model=\"form.date2\"></div><br>\r\n        <div><label>Reason:</label><textarea class=\"textArea\" v-model=\"form.reason\"></textarea></div><br>\r\n        <button type=\"submit\">Submit</button>\r\n      </form>\r\n    </div>\r\n\r\n    <div>\r\n      <pre>{{ goals }}</pre>\r\n      <table class=\"table table-bordered\">\r\n        <thead class=\"table-dark\">\r\n          <tr>\r\n            <th>Employee Name</th>\r\n            <th>Type Leave</th>\r\n            <th>Days</th>\r\n            <th>Reason</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(goal, index) in goals\" :key=\"index\">\r\n            <td>{{ goal.name }}</td>\r\n            <td>{{ goal.leave }}</td>\r\n            <td>{{ goal.days }}</td>\r\n            <td>{{ goal.reason }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      normalLeave: 12,\r\n      sickLeave: 12,\r\n      form: {\r\n        id: \"\",\r\n        name: \"\",\r\n        leave: \"\",\r\n        date1: \"\",\r\n        date2: \"\",\r\n        reason: \"\"\r\n      },\r\n      goals: []\r\n    };\r\n  },\r\n  methods: {\r\n    submit() {\r\n      axios.post(\"https://localhost:7188/api/User/createleave\", {\r\n        emp_id: this.form.id,\r\n        emp_name: this.form.name,\r\n        leaveType: this.form.leave,\r\n        startDate: this.form.date1,\r\n        endDate: this.form.date2,\r\n        days: 6, // or calculate days difference between dates\r\n        status: this.form.reason\r\n      })\r\n      .then(response => {\r\n        this.goals.push(response.data); // assuming the response is an object containing the leave data\r\n        console.log(\"Leave added successfully\");\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error adding leave:\", error);\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.container {\r\n  max-width: 800px;\r\n  margin: auto;\r\n}\r\n\r\n.leaves {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.leavesBox {\r\n  border: 1px solid #ccc;\r\n  padding: 10px;\r\n  width: 200px;\r\n  text-align: center;\r\n}\r\n\r\n.form {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form label {\r\n  width: 100px;\r\n  display: inline-block;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textarea {\r\n  width: calc(100% - 100px);\r\n  padding: 8px;\r\n  box-sizing: border-box;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.form button {\r\n  background-color: #4caf50;\r\n  color: white;\r\n  padding: 10px 15px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.form button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.table th, .table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.table th {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":";AA8DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE;QACJC,EAAE,EAAE,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE;MACV,CAAC;MACDC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPb,KAAK,CAACc,IAAI,CAAC,6CAA6C,EAAE;QACxDC,MAAM,EAAE,IAAI,CAACX,IAAI,CAACC,EAAE;QACpBW,QAAQ,EAAE,IAAI,CAACZ,IAAI,CAACE,IAAI;QACxBW,SAAS,EAAE,IAAI,CAACb,IAAI,CAACG,KAAK;QAC1BW,SAAS,EAAE,IAAI,CAACd,IAAI,CAACI,KAAK;QAC1BW,OAAO,EAAE,IAAI,CAACf,IAAI,CAACK,KAAK;QACxBW,IAAI,EAAE,CAAC;QAAE;QACTC,MAAM,EAAE,IAAI,CAACjB,IAAI,CAACM;MACpB,CAAC,EACAY,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACZ,KAAK,CAACa,IAAI,CAACD,QAAQ,CAACtB,IAAI,CAAC,EAAE;QAChCwB,OAAO,CAACC,GAAG,CAAC,0BAA0B,CAAC;MACzC,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAAC;MAC7C,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}