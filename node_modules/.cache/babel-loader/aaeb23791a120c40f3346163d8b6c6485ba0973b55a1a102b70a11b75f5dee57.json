\n    return {\r\n      normalLeave: 12,\r\n      sickLeave: 12,\r\n      form: {\r\n        name: \"\",\r\n        textArea: \"\",\r\n        leave: \"\",\r\n        date1:\"\",\r\n      date2: \"\",\r\n      },\r\n      goals: [],\r\n      users:'',\r\n    };\r\n     \r\n  },\r\n  methods: {\r\nsubmit() {\r\n  const start = new Date(this.form.date1);\r\n  const end = new Date(this.form.date2);\r\n  const diff = (end - start) / (1000 * 60 * 60 * 24)+ 1;//difference between end date and from date\r\n  this.goals.push({ ...this.form, day: diff });\r\n  if(this.form.name=== ''){\r\n    alert(\"please enter the name\")\r\n  }\r\n  else{\r\n  if (this.form.leave === \"Normal Leave\" && diff > 0) {\r\n    this.normalLeave -= diff;\r\n  } else if (this.form.leave === \"Sick Leave\" && diff > 0) {\r\n    this.sickLeave -= diff;\r\n  }\r\n\r\n  this.form = {\r\n    name: \"\",\r\n    textArea: \"\",\r\n    leave: \"\",\r\n    date1: \"\",\r\n    date2: \"\",\r\n  };\r\n}\r\n},\r\n  }\r\n  \r\n};\r\n</script> \r\n<style>\r\n.form {\r\n  max-width: 400px;\r\n  margin: auto;\r\n  font-family: 'Arial', sans-serif;\r\n}\r\n\r\n.form label {\r\n  display: block;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textArea {\r\n  width: 100%;\r\n  padding: 8px;\r\n  margin-bottom: 16px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.form button {\r\n  background-color: #4caf50;\r\n  color: white;\r\n  padding: 10px 15px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.form button:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n\r\n.select {\r\n  width: 220px;\r\n  height: 30px;\r\n  border-radius: 10px;\r\n  margin: 5px;\r\n  display: inline-block;\r\n}\r\n\r\n.leaves {\r\n  /* border: 1px solid; */\r\n  width: 500px;\r\n  display: flex;\r\n  height: 150px;\r\n  padding-top: 20px;\r\n  padding-left: 80px;\r\n  margin-left: 40px;\r\n  justify-content: space-between;\r\n}\r\n\r\n.leavesBox1 {\r\n  border: 1px solid;\r\n  margin-b