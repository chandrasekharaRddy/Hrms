{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      users: []\n    };\n  },\n  methods: {\n    login() {\n      axios.get(\"http://localhost:5296/api/Todoapp/GetNotes\").then(response => {\n        this.users = response.data;\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n      const user = this.users.find(user => user.name === this.username && user.password === this.password);\n      if (user) {\n        const userData = {\n          details: user.details.map(detail => ({\n            Date: detail.Date,\n            InTime1: detail.InTime1,\n            OutTime1: detail.OutTime1,\n            InTime2: detail.InTime2,\n            OutTime2: detail.OutTime2\n          })),\n          employeeName: user.employeeName\n        };\n        this.$store.dispatch('setUserData', userData);\n        this.$router.push({\n          name: 'navigation'\n        });\n      } else {\n        alert(\"Wrong credentials\");\n      }\n      this.username = \"\";\n      this.password = \"\";\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","users","methods","login","get","then","response","catch","error","console","user","find","name","userData","details","map","detail","Date","InTime1","OutTime1","InTime2","OutTime2","employeeName","$store","dispatch","$router","push","alert"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\HrmsLoginLevel.vue"],"sourcesContent":["<template>\r\n    <div class=\"login-box\">\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"user-box\">\r\n        <input type=\"text\" name=\"\"  v-model=\"username\" >\r\n        <label>Username</label>\r\n      </div>\r\n        <div class=\"user-box\">\r\n        <input type=\"password\" name=\"\"  v-model=\"password\" >\r\n        <label>Password</label>\r\n      <