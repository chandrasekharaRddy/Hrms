{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      form: {\n        id: \"\",\n        name: \"\",\n        leave: \"\",\n        date1: \"\",\n        date2: \"\",\n        status: \"\",\n        email: \"\"\n      },\n      goals: []\n    };\n  },\n  methods: {\n    submit() {\n      const start = new Date(this.form.date1);\n      const end = new Date(this.form.date2);\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1;\n      axios.post(\"https://localhost:7188/api/User/createleave\", {\n        emp_id: this.form.id,\n        emp_name: this.form.name,\n        leaveType: this.form.leave,\n        startDate: this.form.date1,\n        endDate: this.form.date2,\n        status: this.form.status,\n        email: this.form.email\n      }).then(response => {\n        const leaveDetails = response.data;\n        console.log(\"Leave Details:\", leaveDetails);\n        this.goals.push({\n          ...this.form,\n          day: diff\n        });\n        this.clearForm();\n      }).catch(error => {\n        console.error(\"Error adding leave:\", error);\n        // Handle error appropriately, e.g., show error message to user\n      });\n    },\n    clearForm() {\n      this.form.id = \"\";\n      this.form.name = \"\";\n      this.form.leave = \"\";\n      this.form.date1 = \"\";\n      this.form.date2 = \"\";\n      this.form.status = \"\";\n      this.form.email = \"\";\n    }\n  }\n};","map":{"version":3,"names":["axios","data","form","id","name","leave","date1","date2","status","email","goals","methods","submit","start","Date","end","diff","post","emp_id","emp_name","leaveType","startDate","endDate","then","response","leaveDetails","console","log","push","day","clearForm","catch","error"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\leaveForm.vue"],"sourcesContent":["\r\n\r\n<template>\r\n \r\n  <!-- <div class=\"leaves\">\r\n    <div cla