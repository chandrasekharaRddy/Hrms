ordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>EmployeeId</th>\r\n          <th>EmployeeName</th>\r\n          <th>StartDate</th>\r\n          <th>EndDate</th>\r\n          <th>leaveType</th>\r\n          <th>Status</th>\r\n          <th>No Of Days</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(goal,index) in goals\" :key=\"index\">\r\n          <td>{{ goal.emp_id }}</td>\r\n          <td>{{ goal.emp_name }}</td>\r\n          <td>{{ goal.startDate}}</td>\r\n          <td>{{ goal.endDate }}</td>\r\n          <td>{{ goal.leaveType}}</td>\r\n          <td>{{ goal.status}}</td>\r\n          <td>{{ goal.day}}</td>\r\n      \r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        id: \"\",\r\n        name: \"\",\r\n        leave: \"\",\r\n        date1: \"\",\r\n        date2: \"\",\r\n        status: \"\",\r\n        email: \"\"\r\n      },\r\n      goals: []\r\n    };\r\n  },\r\n  methods: {\r\n    submit() {\r\n      const start = new Date(this.form.date1);\r\n      const end = new Date(this.form.date2);\r\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1;\r\n      \r\n      axios.post(\"https://localhost:7188/api/User/createleave\", {\r\n        emp_id: this.form.id,\r\n        emp_name: this.form.name,\r\n        leaveType: this.form.leave,\r\n        startDate: this.form.date1,\r\n        endDate: this.form.date2,\r\n        status: this.form.status,\r\n        email: this.form.email\r\n      })\r\n      .then(response => {\r\n        const leaveDetails = response.data;\r\n        console.log(\"Leave Details:\", leaveDetails);\r\n        this.goals.push({ ...this.form, day: diff });\r\n        this.clearForm();\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error adding leave:\", error);\r\n        // Handle error appropriately, e.g., show error message to user\r\n      });\r\n    },\r\n    clearForm() {\r\n      this.form.id = \"\";\r\n      this.form.name = \"\";\r\n      this.form.leave = \"\";\r\n      this.form.date1 = \"\";\r\n      this.form.date2 = \"\";\r\n      this.form.status = \"\";\r\n      this.form.email = \"\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style>\r\n\r\n.form {\r\n  font-family: 'Arial', sans-serif;\r\n  width: 600px;\r\n  background: transparent;\r\n  margin-left: 500px; \r\n  margin-right: auto;\r\n  padding: 20px;\r\n}\r\n.form div {\r\ndisplay: flex;\r\nflex-direction: row;\r\nalign-items: center;\r\nmargin-bottom: 16px;\r\npadding-top: 10px;\r\nwidth: 400px;\r\ncolor:#000;\r\nmargin-right: 150px;\r\n}\r\n\r\n.form label {\r\ndisplay: block;\r\nmargin-right: 3px; \r\nmargin-top: 4px;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textArea {\r\nwidth: 80%;\r\npadding: 10px; \r\nmargin-bottom: 3px; \r\nborder: 1px solid #ccc;\r\nborder-radius: 4px;\r\n}\r\n\r\n.form button {\r\nbackground-color: #4caf50;\r\ncolor: white;\r\npadding: 2px 5px;\r\nborder: none;\r\nborder-radius: 4px;\r\ncursor: pointer;\r\nmargin-left: -250px;\r\n}\r\n\r\n.form button:hover {\r\nbackground-color: #45a049;\r\n}\r\n\r\n.select {\r\nwidth: 250px;\r\nheight: 40px;\r\nborder-radius: 10px;\r\nmargin: 5px;\r\ndisplay: inline-block;\r\n}\r\n\r\n.leaves {\r\n\r\nwidth: 500px;\r\ndisplay: flex;\r\nheight: 150px;\r\npadding-top: 20px;\r\npadding-left: 80px;\r\nmargin-left: 40px;\r\njustify-content: space-between;\r\n}\r\n\r\n.leavesBox1 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nwidth: 150px;\r\nheight: 140px;\r\n}\r\n\r\n.leavesBox2 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nmargin-right: 80px;\r\nwidth: 150px;\r\nheight: 140px;\r\n\r\n}\r\n\r\n.normalLeave {\r\nborder: 1px solid;\r\nwidth: 100px;\r\npadding-bottom: 25px;\r\npadding-top: 10px;\r\nmargin-left: 20px;\r\nmargin-top: 