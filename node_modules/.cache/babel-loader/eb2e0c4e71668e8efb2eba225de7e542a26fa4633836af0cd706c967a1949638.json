{"ast":null,"code":"export default {\n  computed: {\n    userDetails() {\n      // Get the first user details\n      return this.$store.state.userData && this.$store.state.userData.length > 0 ? this.$store.state.userData[0] : null;\n    },\n    employees() {\n      if (this.userData && this.userData.length > 0) {\n        // Filter unique employees based on empId, excluding the first user\n        const uniqueEmployees = this.userData.slice(1).filter((user, index, self) => index === self.findIndex(t => t.empId === user.empId));\n        return uniqueEmployees;\n      }\n      return [];\n    }\n  }\n};","map":{"version":3,"names":["computed","userDetails","$store","state","userData","length","employees","uniqueEmployees","slice","filter","user","index","self","findIndex","t","empId"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\attendenceForm.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- User Table -->\r\n    <table v-if=\"userDetails\" class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>UserId</th>\r\n          <th>Name</th>\r\n          <th>Email</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr>\r\n          <td>{{ userDetails.userId }}</td>\r\n          <td>{{ userDetails.userName }}</td>\r\n          <td>{{ userDetails.email }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Employee Table -->\r\n    <table v-if=\"employees && employees.length > 0\" class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>EmployeeId</th>\r\n          <th>EmployeeName</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(employee, index) in employees\" :key=\"'emp_' + index\">\r\n          <td>{{ employee.empId }}</td>\r\n          <td>{{ employee.empName }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  computed: {\r\n    userDetails() {\r\n      // Get the first user details\r\n      return this.$store.state.userData && this.$store.state.userData.length > 0\r\n        ? this.$store.state.userData[0]\r\n        : null;\r\n    },\r\n    employees() {\r\n      if (this.userData && this.userData.length > 0) {\r\n        // Filter unique employees based on empId, excluding the first user\r\n        const uniqueEmployees = this.userData.slice(1).filter((user, index, self) =>\r\n          index === self.findIndex((t) => (\r\n            t.empId === user.empId\r\n          ))\r\n        );\r\n        return uniqueEmployees;\r\n      }\r\n      return [];\r\n    }\r\n  },\r\n}\r\n</script>\r\n"],"mappings":"AAuCA,eAAe;EACbA,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ;MACA,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,QAAO,IAAK,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,QAAQ,CAACC,MAAK,GAAI,IACrE,IAAI,CAACH,MAAM,CAACC,KAAK,CAACC,QAAQ,CAAC,CAAC,IAC5B,IAAI;IACV,CAAC;IACDE,SAASA,CAAA,EAAG;MACV,IAAI,IAAI,CAACF,QAAO,IAAK,IAAI,CAACA,QAAQ,CAACC,MAAK,GAAI,CAAC,EAAE;QAC7C;QACA,MAAME,eAAc,GAAI,IAAI,CAACH,QAAQ,CAACI,KAAK,CAAC,CAAC,CAAC,CAACC,MAAM,CAAC,CAACC,IAAI,EAAEC,KAAK,EAAEC,IAAI,KACtED,KAAI,KAAMC,IAAI,CAACC,SAAS,CAAEC,CAAC,IACzBA,CAAC,CAACC,KAAI,KAAML,IAAI,CAACK,KAClB,CACH,CAAC;QACD,OAAOR,eAAe;MACxB;MACA,OAAO,EAAE;IACX;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}