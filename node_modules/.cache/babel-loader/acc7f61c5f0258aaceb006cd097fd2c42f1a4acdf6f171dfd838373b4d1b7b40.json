{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      users: []\n    };\n  },\n  methods: {\n    login() {\n      axios.get(\"http://localhost:5296/api/Todoapp/GetNotes\").then(response => {\n        this.users = response.data;\n        const users = this.users.filter(u => u.name === this.username && u.Password === this.password);\n        if (users.length > 0) {\n          const userData = users.map(user => ({\n            name: user.name,\n            Date: user.Date,\n            InTime1: user.Intime1,\n            OutTime1: user.Outtime1,\n            InTime2: user.Intime2,\n            OutTime2: user.Outtime2\n          }));\n          this.$store.dispatch('setUserData', userData);\n          this.$router.push({\n            name: 'navigation'\n          });\n          //   console.log(userData);\n        } else {\n          alert(\"Wrong credentials\");\n        }\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","users","methods","login","get","then","response","filter","u","name","Password","length","userData","map","user","Date","InTime1","Intime1","OutTime1","Outtime1","InTime2","Intime2","OutTime2","Outtime2","$store","dispatch","$router","push","alert","catch","error","console"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\logiForm.vue"],"sourcesContent":["  <template>\r\n  <div class=\"login-box\">\r\n    <form @submit.prevent=\"login\">\r\n      <div class=\"user-box\">\r\n      <input type=\"text\" name=\"\"  v-model=\"username\" >\r\n      <label>Username</label>\r\n    </div>\r\n      <div class=\"user-box\">\r\n      <input type=\"password\" name=\"\"  v-model=\"password\" >\r\n      <label>Password</label>\r\n    </div>\r\n      <br>\r\n  \r\n      <a href=\"#\">\r\n      <span></span>\r\n      <span></span>\r\n      <span></span>\r\n      <span></span>\r\n      <button class=\"button\">Login</button>\r\n    </a></form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        username: \"\",\r\n        password: \"\",\r\n        users: []\r\n      }\r\n    },\r\n    methods: {\r\n      login() {\r\n        axios.get(\"http://localhost:5296/api/Todoapp/GetNotes\")\r\n          .then(response => {\r\n            this.users = response.data;\r\n            const users = this.users.filter(u => u.name === this.username && u.Password === this.password);\r\n            if (users.length > 0) {\r\n              const userData = users.map(user => ({\r\n                name: user.name,\r\n                  Date: user.Date,\r\n                  InTime1: user.Intime1,\r\n                  OutTime1: user.Outtime1,\r\n                  InTime2: user.Intime2,\r\n                  OutTime2: user.Outtime2,\r\n        \r\n              }));\r\n              this.$store.dispatch('setUserData', userData);\r\n             this.$router.push({ name: 'navigation' });\r\n            //   console.log(userData);\r\n            } else {\r\n              alert(\"Wrong credentials\");\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching data:', error);\r\n          });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n<style>\r\nhtml {\r\n  height: 100%;\r\n}\r\nbody {\r\n  margin:0;\r\n  padding:0;\r\n  font-family: sans-serif;\r\n  background: linear-gradient(#141e30,#141e30 );\r\n}\r\n\r\n.login-box {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  width: 400px;\r\n  padding: 40px;\r\n  transform: translate(-50%, -50%);\r\n  background: #141e30;\r\n  box-sizing: border-box;\r\n  box-shadow: 0 15px 25px rgba(148, 180, 106, 0.6);\r\n  border-radius: 10px;\r\n}\r\n\r\n.login-box h2 {\r\n  margin: 0 0 3