{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      normalLeave: 12,\n      sickLeave: 12,\n      form: {\n        name: \"\",\n        textArea: \"\",\n        leave: \"\",\n        date1: \"\",\n        date2: \"\"\n      },\n      goals: [],\n      users: ''\n    };\n  },\n  methods: {\n    submit() {\n      const start = new Date(this.form.date1);\n      const end = new Date(this.form.date2);\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1; //difference between end date and from date\n      this.goals.push({\n        ...this.form,\n        day: diff\n      });\n      if (this.form.name === '') {\n        alert(\"please enter the name\");\n      } else {\n        if (this.form.leave === \"Normal Leave\" && diff > 0) {\n          this.normalLeave -= diff;\n        } else if (this.form.leave === \"Sick Leave\" && diff > 0) {\n          this.sickLeave -= diff;\n        }\n        this.form = {\n          name: \"\",\n          textArea: \"\",\n          leave: \"\",\n          date1: \"\",\n          date2: \"\"\n        };\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","normalLeave","sickLeave","form","name","textArea","leave","date1","date2","goals","users","methods","submit","start","Date","end","diff","push","day","alert"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\hrmsLeaveLevel.vue"],"sourcesContent":["\r\n<template>\r\n  <div class=\"leaves\">\r\n    <div class=\"leavesBox1\">\r\n      <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" fill=\"currentColor\" class=\"bi bi-calendar\" viewBox=\"0 0 16 16\">\r\n        <path d=\"M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5M1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4z\" />\r\n      </svg> <br><label class=\"label\">NormalLeaves</label>\r\n      <div class=\"normalLeave\">{{ normalLeave }}</di