n      textArea: \"\",\r\n      leave: \"\",\r\n      date1:\"\",\r\n    date2: \"\",\r\n    },\r\n    goals: [],\r\n    users:'',\r\n  };\r\n   \r\n},\r\nmethods: {\r\nsubmit() {\r\nconst start = new Date(this.form.date1);\r\nconst end = new Date(this.form.date2);\r\nconst diff = (end - start) / (1000 * 60 * 60 * 24)+ 1;//difference between end date and from date\r\nthis.goals.push({ ...this.form, day: diff });\r\nif(this.form.name=== ''){\r\n  alert(\"please enter the name\")\r\n}\r\nelse{\r\nif (this.form.leave === \"Normal Leave\" && diff > 0) {\r\n  this.normalLeave -= diff;\r\n} else if (this.form.leave === \"Sick Leave\" && diff > 0) {\r\n  this.sickLeave -= diff;\r\n}\r\n\r\nthis.form = {\r\n  name: \"\",\r\n  textArea: \"\",\r\n  leave: \"\",\r\n  date1: \"\",\r\n  date2: \"\",\r\n};\r\n}\r\n},\r\n}\r\n\r\n};\r\n</script> \r\n<style>\r\n.form {\r\nmax-width: 400px;\r\nmargin: auto;\r\nfont-family: 'Arial', sans-serif;\r\n}\r\n\r\n.form div {\r\ndisplay: flex;\r\nflex-direction: row;\r\nalign-items: center;\r\nmargin-bottom: 16px;\r\n}\r\n\r\n.form label {\r\nflex: 1;\r\nmargin-right: 10px;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textArea {\r\nflex: 2;\r\npadding: 8px;\r\nbox-sizing: border-box;\r\n}\r\n\r\n.form button {\r\nbackground-color: #4caf50;\r\ncolor: white;\r\npadding: 10px 15px;\r\nborder: none;\r\nborder-radius: 4px;\r\ncursor: pointer;\r\n}\r\n\r\n.form button:hover {\r\nbackground-color: #45a049;\r\n}\r\n\r\n.select {\r\nwidth: 250px;\r\nheight: 40px;\r\nborder-radius: 10px;\r\nmargin: 5px;\r\ndisplay: inline-block;\r\n}\r\n\r\n.leaves {\r\n/* border: 1px solid; */\r\nwidth: 500px;\r\ndisplay: flex;\r\nheight: 150px;\r\npadding-top: 20px;\r\npadding-left: 80px;\r\nmargin-left: 40px;\r\njustify-content: space-between;\r\n}\r\n\r\n.leavesBox1 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nwidth: 150px;\r\nheight: 140px;\r\n}\r\n\r\n.leavesBox2 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nmargin-right: 80px;\r\nwidth: 150px;\r\nheight: 140px;\r\n\r\n}\r\n\r\n.normalLeave {\r\nborder: 1px solid;\r\nwidth: 100px;\r\npadding-bottom: 25px;\r\npadding-top: 10px;\r\nmargin-left: 20px;\r\nmargin-top: 10px;\r\nheight: 40px;\r\nbackground-color: coral;\r\n}\r\n\r\n.label {\r\ncolor: rgb(13, 128, 235);\r\n\r\n}\r\n.textArea{\r\nborder: ridge 2px;\r\npadding: 5px;\r\nwidth: 15em;\r\nmin-height: em;\r\noverflow: auto;\r\nmargin: 5px;\r\ndisplay: inline-block;\r\n}\r\ndiv.form{\r\nwidth: 12cm;\r\npadding:10px 20px;\r\nborder-radius: 10px;\r\n}\r\nlabel{\r\nwidth :140px;\r\ndisplay: inline-block;\r\nmargin: 0.2cm;\r\nfont-size: 16px;\r\nfont-weight:bold;\r\n}\r\ninput{\r\npadding: 4px;\r\nmargin: 5px;\r\nfont-size: 16px;\r\nborder-radius: 10px;\r\n}\r\n\r\n</style>"],"mappings":";AAuDA,eAAe;EACfA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAC,EAAE;QACVC,KAAK,EAAE;MACP,CAAC;MACDC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAC;IACR,CAAC;EAEH,CAAC;EACDC,OAAO,EAAE;IACTC,MAAMA,CAAA,EAAG;MACT,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,IAAI,CAACX,IAAI,CAACI,KAAK,CAAC;MACvC,MAAMQ,GAAE,GAAI,IAAID,IAAI,CAAC,IAAI,CAACX,IAAI,CAACK,KAAK,CAAC;MACrC,MAAMQ,IAAG,GAAI,CAACD,GAAE,GAAIF,KAAK,KAAK,IAAG,GAAI,EAAC,GAAI,EAAC,GAAI,EAAE,CAAC,GAAE,CAAC,CAAC;MACtD,IAAI,CAACJ,KAAK,CAACQ,IAAI,CAAC;QAAE,GAAG,IAAI,CAACd,IAAI;QAAEe,GAAG,EAAEF;MAAK,CAAC,CAAC;MAC5C,IAAG,IAAI,CAACb,IAAI,CAACC,IAAI,KAAI,EAAE,EAAC;QACtBe,KAAK,CAAC,uBAAuB;MAC/B,OACI;QACJ,IAAI,IAAI,CAAChB,IAAI,CAACG,KAAI,KAAM,cAAa,IAAKU,IAAG,GAAI,CAAC,EAAE;UAClD,IAAI,CAACf,WAAU,IAAKe,IAAI;QAC1B,OAAO,IAAI,IAAI,CAACb,IAAI,CAACG,KAAI,KAAM,YAAW,IAAKU,IAAG,GAAI,CAAC,EAAE;UACvD,IAAI,CAACd,SAAQ,IAAKc,IAAI;QACxB;QAEA,IAAI,CAACb,IAAG,GAAI;UACVC,IAAI,EAAE,EAAE;UACRC,QAAQ,EAAE,EAAE;UACZC,KAAK,EAAE,EAAE;UACTC,KAAK,EAAE,EAAE;UACTC,KAAK,EAAE;QACT,CAAC;MACD;IACA;EACA;AAEA,CAAC"},"metadata":{},"sourceType":"module"