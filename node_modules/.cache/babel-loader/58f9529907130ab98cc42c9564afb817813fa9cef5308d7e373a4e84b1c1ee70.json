<br>\r\n      <label for=\"reason\">Reason:</label>\r\n      <textarea id=\"reason\" v-model=\"reason\"></textarea><br>\r\n      <button type=\"submit\">Submit Leave Request</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      normalLeaveBalance: 0,\r\n      sickLeaveBalance: 0,\r\n      leaveType: 'Normal Leave',\r\n      startDate: '',\r\n      endDate: '',\r\n      reason: ''\r\n    };\r\n  },\r\n  mounted() {\r\n    // Fetch remaining leave balances on component mount\r\n    this.fetchRemainingLeaveBalances();\r\n  },\r\n  methods: {\r\n    async fetchRemainingLeaveBalances() {\r\n      try {\r\n        // Make API request to fetch remaining leave balances\r\n        const response = await this.$axios.get('/api/user/leavebalances');\r\n        this.normalLeaveBalance = response.data.normalLeaveBalance;\r\n        this.sickLeaveBalance = response.data.sickLeaveBalance;\r\n      } catch (error) {\r\n        console.error('Error fetching remaining leave balances:', error);\r\n        // Handle error\r\n      }\r\n    },\r\n    async submitLeaveRequest() {\r\n      try {\r\n        // Prepare leave request data\r\n        const leaveRequestData = {\r\n          leaveType: this.leaveType,\r\n          startDate: this.startDate,\r\n          endDate: this.endDate,\r\n          reason: this.reason\r\n        };\r\n\r\n        // Make API request to create leave request\r\n        const response = await this.$axios.post('/api/user/createleave', leaveRequestData);\r\n        console.log('Leave request created:', response.data);\r\n        // Optionally, show success message or redirect to another page\r\n      } catch (error) {\r\n        console.error('Error creating leave request:', error);\r\n        // Handle error\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":";gCA6RIA,mBAAA,CAA2B,YAAvB,oBAAkB;gCACyCA,mBAAA,CAAI;gCACRA,mBAAA,CAAI;gCAE7DA,mBAAA,CAA0C;EAAnCC,GAAG,EAAC;AAAW,GA