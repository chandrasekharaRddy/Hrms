{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: \"\",\n      password: \"\",\n      users: []\n    };\n  },\n  methods: {\n    async login() {\n      await axios.get('https://localhost:7188/api/User/authenticat', {\n        params: {\n          userName: 'this.userName',\n          Password: 'this.password'\n        }\n      }).then(response => {\n        console.log(response.data);\n        const userData = response.data.users;\n        const employeeData = response.data.employees;\n        const user = userData.find(u => u.userName === this.username && u.password === this.password);\n        if (user) {\n          const userData = {\n            name: user.name,\n            id: user.userId,\n            email: user.email\n          };\n          this.$store.dispatch('setUserData', {\n            userData,\n            employeeData\n          });\n          this.$router.push({\n            name: 'navigation'\n          });\n        } else {\n          alert(\"Wrong credentials\");\n        }\n      }).catch(error => {\n        console.error('Error fetching data:', error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","users","methods","login","get","params","userName","Password","then","response","console","log","userData","employeeData","employees","user","find","u","name","id","userId","email","$store","dispatch","$router","push","alert","catch","error"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\logiForm.vue"],"sourcesContent":["  <template>\r\n  <div class=\"login-box\">\r\n    <form @submit.prevent=\"login\">\r\n      <div class=\"user-box\">\r\n      <input type=\"text\" name=\"\"  v-model=\"username\" >\r\n      <label>Username</label>\r\n    </div>\r\n      <div class=\"user-box\">\r\n      <input type=\"password\" name=\"\"  v-model=\"password\" >\r\n      <label>Password</label>\r\n    </div>\r\n      <br>\r\n  \r\n      <a href=\"#\">\r\n      <span></span>\r\n      <span></span>\r\n      <span></span>\r\n      <span></span>\r\n      <button class=\"button\">Login</button>\r\n    </a></form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        username: \"\",\r\n        password: \"\",\r\n        users: []\r\n      }\r\n    },\r\n    methods: {\r\n     async login() {\r\n       await axios.get('https://localhost:7188/api/User/authenticat', {\r\n          params: {\r\n            userName: 'this.userName',\r\n            Password: 'this.password'\r\n          }\r\n     })\r\n          .then(response => {\r\n            console.log(response.data)\r\n            const userData = response.data.users;\r\n            const employeeData= response.data.employees;\r\n            \r\n            \r\n            const user = userData.find(u => u.userName === this.username && u.password === this.password);\r\n            if (user) {\r\n             \r\n              const userData =  {\r\n                name: user.name,\r\n                  id: user.userId,\r\n                  email:user.email\r\n              };\r\n\r\n             \r\n              this.$store.dispatch('setUserData', {userData,employeeData});\r\n             this.$router.push({ name: 'navigation' });\r\n  \r\n            } else {\r\n              alert(\"Wrong credentials\");\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching data:', error);\r\n          });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n<style>\r\nhtml {\r\n  height: 100%;\r\n}\r\nbody {\r\n  margin:0;\r\n  padding:0;\r\n  font-family: sans-serif;\r\n  background: linear-gradient(#141e30,#141e30 );\r\n}\r\n\r\n.login-box {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  width: 400px;\r\n  padding: 40px;\r\n  transform: translate(-50%, -50%);\r\n