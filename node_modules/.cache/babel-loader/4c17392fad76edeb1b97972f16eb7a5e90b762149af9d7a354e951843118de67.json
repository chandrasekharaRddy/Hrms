{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      form: {\n        id: \"\",\n        name: \"\",\n        leave: \"\",\n        date1: \"\",\n        date2: \"\",\n        email: \"\",\n        reason: \"\"\n      },\n      goals: [],\n      normalLeave: 12,\n      sickLeave: 12\n    };\n  },\n  methods: {\n    async submit() {\n      const start = new Date(this.form.date1);\n      const end = new Date(this.form.date2);\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1;\n      try {\n        const response = await axios.post('https://localhost:7188/api/User/createleave', {\n          LeaveType: this.form.leave,\n          StartDate: this.form.date1,\n          EndDate: this.form.date2,\n          Status: this.form.reason,\n          Days: diff,\n          emp_name: this.form.name,\n          email: this.form.email\n        });\n        const leaveDetails = response.data;\n        this.goals.push({\n          leaveDetails,\n          day: diff\n        });\n        if (this.form.leave === \"Normal Leave\" && diff > 0) {\n          this.normalLeave -= diff;\n        } else if (this.form.leave === \"Sick Leave\" && diff > 0) {\n          this.sickLeave -= diff;\n        }\n      } catch (error) {\n        alert(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","form","id","name","leave","date1","date2","email","reason","goals","normalLeave","sickLeave","methods","submit","start","Date","end","diff","response","post","LeaveType","StartDate","EndDate","Status","Days","emp_name","leaveDetails","push","day","error","alert"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\leaveForm.vue"],"sourcesContent":["\r\n\r\n<template>\r\n \r\n  <div class=\"leaves\">\r\n    <div class=\"leavesBox1\">\r\n      <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" fill=\"currentColor\" class=\"bi bi-calendar\" vie