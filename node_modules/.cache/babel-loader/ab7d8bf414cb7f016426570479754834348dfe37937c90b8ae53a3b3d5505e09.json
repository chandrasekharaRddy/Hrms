{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  computed: {\n    userDetails() {\n      return this.$store.state.userData && this.$store.state.userData.length > 0 ? this.$store.state.userData[0] : null;\n    }\n  },\n  methods: {\n    async submitLeave() {\n      try {\n        if (!this.userDetails) {\n          alert('User details not available.');\n          return;\n        }\n        const {\n          username,\n          password\n        } = this.userDetails;\n        const response = await axios.post('https://yourapi.com/api/user/createleave', {\n          LeaveType: this.leaveType,\n          StartDate: this.startDate,\n          EndDate: this.endDate\n        }, {\n          params: {\n            userName: username,\n            password: password\n          }\n        });\n        console.log(response.data);\n        alert('Leave request created successfully!');\n      } catch (error) {\n        console.error(error);\n        alert('An error occurred while creating leave request.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","computed","userDetails","$store","state","userData","length","methods","submitLeave","alert","username","password","response","post","LeaveType","leaveType","StartDate","startDate","EndDate","endDate","params","userName","console","log","data","error"],"sources":["C:\\Users\\IMPEGNOABS010\\Desktop\\HRMSROUT\\src\\components\\leaveForm.vue"],"sourcesContent":["<!-- \r\n\r\n<template>\r\n \r\n  <div class=\"leaves\">\r\n    <div class=\"leavesBox1\">\r\n      <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" fill=\"currentColor\" class=\"bi bi-calendar\" viewBox=\"0 0 16 16\">\r\n        <path d=\"M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5M1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4z\" />\r\n      </svg> <br><label class=\"label\">NormalLeaves</label>\r\n      <div class=\"normalLeave\">{{ normalLeave }}</div>\r\n    </div><br>\r\n    <div class=\"leavesBox2\">\r\n      <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" fill=\"currentColor\" class=\"bi bi-calendar\" viewBox=\"0 0 16 16\">\r\n        <path d=\"M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5M1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4z\" />\r\n      </svg><br><label class=\"label\">SickLeaves</label>\r\n      <div class=\"normalLeave\">{{ sickLeave }}</div>\r\n    </div>\r\n  </div>\r\n  \r\n  <div class=\"form\">\r\n    <form v-on:submit.prevent=\"submit\">\r\n      <div><label>Employee ID:</label><input type=\"text\" v-model=\"form.id\"></div>\r\n      <div><label>Name:</label><input type=\"text\" v-model=\"form.name\"></div>\r\n      <div>\r\n        <label>Type Leave:</label>\r\n        <select class=\"select\" v-model=\"form.leave\">\r\n          <option>Normal Leave</option>\r\n          <option>Sick Leave</option>z\r\n        </select>\r\n      </div>\r\n      <div><label>From:</label><input type=\"date\" v-model=\"form.date1\"></div>\r\n      <div><label>To:</label><input type=\"date\" v-model=\"form.date2\"></div>\r\n      <div><label>Email:</label><input type=\"email\" v-model=\"form.email\"></div><br>\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n\r\n  <div>\r\n    <table class=\"table table-bordered\">\r\n      <thead class=\"table-dark\">\r\n        <tr>\r\n          <th>Employee ID</th>\r\n          <th>Employee Name</th>\r\n          <th>Start Date</th>\r\n          <th>End Date</th>\r\n          <th>Leave Type</th>\r\n          <th>No Of Days</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(item, index) in goals\" :key=\"index\">\r\n          <td>{{ item.leaveDetails.emp_id }}</td>\r\n          <td>{{ item.leaveDetails.emp_name }}</td>\r\n          <td>{{ item.leaveDetails.startDate }}</td>\r\n          <td>{{ item.leaveDetails.endDate }}</td>\r\n          <td>{{ item.leaveDetails.leaveType }}</td>\r\n          <td>{{ item.day }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        id: \"\",\r\n        name: \"\",\r\n        leave: \"\",\r\n        date1: \"\",\r\n        date2: \"\",\r\n        email: \"\"\r\n      },\r\n      goals: [],\r\n      normalLeave: 12,\r\n        sickLeave: 12,\r\n    };\r\n  },\r\n  methods: {\r\n    submit() {\r\n      const start = new Date(this.form.date1);\r\n      const end = new Date(this.form.date2);\r\n      const diff = (end - start) / (1000 * 60 * 60 * 24) + 1;\r\n\r\n      axios.post(\"https://localhost:7188/api/User/createleave\", {\r\n        emp_id: this.form.id,\r\n        emp_name: this.form.name,\r\n        leaveType: this.form.leave,\r\n        startDate: this.form.date1,\r\n        endDate: this.form.date2,\r\n        email: this.form.email,\r\n        days:diff\r\n      })\r\n      .then(response => {\r\n        const leaveDetails = response.data;\r\n        console.log(\"Leave Details:\", leaveDetails);\r\n        this.goals.push({ leaveDetails, day: diff });\r\n\r\n        if (this.form.leave === \"Normal Leave\" && diff > 0) {\r\n      this.normalLeave -= diff;\r\n    } else if (this.form.leave === \"Sick Leave\" && diff > 0) {\r\n      this.sickLeave -= diff;\r\n    }\r\n\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error adding leave:\", error);\r\n        \r\n      });\r\n    },\r\n    \r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.form {\r\n  font-family: 'Arial', sans-serif;\r\n  width: 600px;\r\n  background: transparent;\r\n  margin-left: 500px; \r\n  margin-right: auto;\r\n  padding: 20px;\r\n}\r\n.form div {\r\ndisplay: flex;\r\nflex-direction: row;\r\nalign-items: center;\r\nmargin-bottom: 16px;\r\npadding-top: 10px;\r\nwidth: 400px;\r\ncolor:#000;\r\nmargin-right: 150px;\r\n}\r\n\r\n.form label {\r\ndisplay: block;\r\nmargin-right: 3px; \r\nmargin-top: 4px;\r\n}\r\n\r\n.form input,\r\n.form select,\r\n.form textArea {\r\nwidth: 80%;\r\npadding: 10px; \r\nmargin-bottom: 3px; \r\nborder: 1px solid #ccc;\r\nborder-radius: 4px;\r\n}\r\n\r\n.form button {\r\nbackground-color: #4caf50;\r\ncolor: white;\r\npadding: 2px 5px;\r\nborder: none;\r\nborder-radius: 4px;\r\ncursor: pointer;\r\nmargin-left: -250px;\r\n}\r\n\r\n.form button:hover {\r\nbackground-color: #45a049;\r\n}\r\n\r\n.select {\r\nwidth: 250px;\r\nheight: 40px;\r\nborder-radius: 10px;\r\nmargin: 5px;\r\ndisplay: inline-block;\r\n}\r\n\r\n.leaves {\r\n\r\nwidth: 500px;\r\ndisplay: flex;\r\nheight: 150px;\r\npadding-top: 20px;\r\npadding-left: 80px;\r\nmargin-left: 40px;\r\njustify-content: space-between;\r\n}\r\n\r\n.leavesBox1 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nwidth: 150px;\r\nheight: 140px;\r\n}\r\n\r\n.leavesBox2 {\r\nborder: 1px solid;\r\nmargin-bottom: 15px;\r\npadding-top: 10px;\r\npadding-left: 4px;\r\nmargin-right: 80px;\r\nwidth: 150px;\r\nheight: 140px;\r\n\r\n}\r\n\r\n.normalLeave {\r\nborder: 1px solid;\r\nwidth: 100px;\r\npadding-bottom: 25px;\r\npadding-top: 10px;\r\nmargin-left: 20px;\r\nmargin-top: 10px;\r\nheight: 40px;\r\nbackground-color: coral;\r\n}\r\n\r\n.label {\r\ncolor: rgb(13, 128, 235);\r\n\r\n}\r\n.textArea{\r\nborder: ridge 2px;\r\npadding: 5px;\r\nwidth: 15em;\r\nmin-height: em;\r\noverflow: auto;\r\nmargin: 5px;\r\ndisplay: inline-block;\r\n}\r\ndiv.form{\r\nwidth: 800px;\r\npadding:10px 20px;\r\nborder-radius: 10px;\r\n}\r\nlabel{\r\nwidth :140px;\r\ndisplay: inline-block;\r\nmargin: 0.2cm;\r\nfont-size: 16px;\r\nfont-weight:bold;\r\n}\r\ninput{\r\npadding: 4px;\r\nmargin: 5px;\r\nfont-size: 16px;\r\nborder-radius: 10px;\r\n}\r\n#tbl tr {\r\n  background: #ccc;\r\n}\r\n\r\n</style> -->\r\n<template>\r\n  <div id=\"app\">\r\n    <h1>Create Leave Request</h1>\r\n    <form @submit.prevent=\"submitLeave\">\r\n      <label for=\"leaveType\">Leave Type:</label>\r\n      <input type=\"text\" id=\"leaveType\" v-model=\"leaveType\" required><br><br>\r\n\r\n      <label for=\"startDate\">Start Date:</label>\r\n      <input type=\"date\" id=\"startDate\" v-model=\"startDate\" required><br><br>\r\n\r\n      <label for=\"endDate\">End Date:</label>\r\n      <input type=\"date\" id=\"endDate\" v-model=\"endDate\" required><br><br>\r\n\r\n      <button type=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  computed: {\r\n    userDetails() {\r\n      return this.$store.state.userData && this.$store.state.userData.length > 0\r\n        ? this.$store.state.userData[0]\r\n        : null;\r\n    },\r\n  },\r\n  methods: {\r\n    async submitLeave() {\r\n      try {\r\n        if (!this.userDetails) {\r\n          alert('User details not available.');\r\n          return;\r\n        }\r\n        \r\n        const { username, password } = this.userDetails;\r\n\r\n        const response = await axios.post(\r\n          'https://yourapi.com/api/user/createleave',\r\n          {\r\n            LeaveType: this.leaveType,\r\n            StartDate: this.startDate,\r\n            EndDate: this.endDate\r\n          },\r\n          {\r\n            params: {\r\n              userName: username,\r\n              password: password\r\n            }\r\n          }\r\n        );\r\n\r\n        console.log(response.data);\r\n        alert('Leave request created successfully!');\r\n      } catch (error) {\r\n        console.error(error);\r\n        alert('An error occurred while creating leave request.');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>"],"mappings":"AAkRA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,QAAO,IAAK,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,QAAQ,CAACC,MAAK,GAAI,IACrE,